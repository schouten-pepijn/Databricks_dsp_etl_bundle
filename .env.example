# Copy to .env and fill values. Do NOT commit .env.
# Databricks workspace authentication (used by Terraform provider)
DATABRICKS_HOST="https://dbc-xxx.cloud.databricks.com"
DATABRICKS_TOKEN="dapi_XXXXXXXXXXXXXXXXXXXXXXXX"

# Bundle root path bases
# Dev: typically under the current user's workspace area
BUNDLE_ROOT_PATH_BASE_DEV="/Workspace/Users"
# Prod: typically under Shared workspace area
BUNDLE_ROOT_PATH_BASE_PROD="/Workspace/Shared"
